<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Servlet_Datos</servlet-name>
        <servlet-class>Servlet.Servlet_Datos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_Categorias</servlet-name>
        <servlet-class>Servlet.Servlet_Categorias</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_Marcas</servlet-name>
        <servlet-class>Servlet.Servlet_Marcas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_Medidas</servlet-name>
        <servlet-class>Servlet.Servlet_Tipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_Proveedores</servlet-name>
        <servlet-class>Servlet.Servlet_Proveedores</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet_Datos</servlet-name>
        <url-pattern>/Servlet_Datos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_Categorias</servlet-name>
        <url-pattern>/Servlet_Categorias</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_Marcas</servlet-name>
        <url-pattern>/Servlet_Marcas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_Medidas</servlet-name>
        <url-pattern>/Servlet_Medidas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_Proveedores</servlet-name>
        <url-pattern>/Servlet_Proveedores</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
